{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"paths": {
			"#app/*": ["./src/app/*"],
			"#server/*": ["./src/server/*"],
			"#shared/*": ["./src/shared/*"],
			"#www/*": ["./src/www/*"]
		},
		"plugins": [
			{
				"name": "@astrojs/ts-plugin"
			}
		]
	},
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist", "vitest.config.ts", "tilly-worktree"]
}
